{
  "name": "search-console-cli",
  "version": "1.0.0",
  "description": "Google Search Console CLI with optional BigQuery support",
  "type": "module",
  "main": "src/cli/index.js",
  "bin": {
    "analytics-cli": "src/cli/index.js"
  },
  "scripts": {
    "start": "node src/cli/index.js",
    "dev": "node --watch src/cli/index.js",
    "api": "node src/api/server.js",
    "api:dev": "node --watch src/api/server.js",
    "api:jwt": "node src/api/server-jwt.js",
    "api:jwt:dev": "node --watch src/api/server-jwt.js",
    "test": "vitest run",
    "test:setup": "node test-setup.js",
    "lint": "eslint .",
    "format": "prettier -w .",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "search-console",
    "gsc",
    "bigquery",
    "cli",
    "inquirer"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@google-cloud/bigquery": "^7.5.0",
    "@googleapis/searchconsole": "^3.0.0",
    "bcryptjs": "^2.4.3",
    "better-sqlite3": "^12.4.1",
    "chalk": "^5.3.0",
    "commander": "^11.1.0",
    "cors": "^2.8.5",
    "csv-stringify": "^6.4.4",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "googleapis": "^160.0.0",
    "inquirer": "^9.3.8",
    "jsonwebtoken": "^9.0.2",
    "open": "^10.2.0",
    "ora": "^7.0.1"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "eslint": "^9.0.0",
    "prettier": "^3.1.0",
    "typescript": "^5.3.0",
    "vitest": "^3.2.4"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
